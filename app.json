{
  "expo": {
    "name": "handbook",
    "slug": "handbook",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/post-it-icon.png",
    "splash": {
      "image": "./assets/post-it-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/post-it-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.silent2code.handbook"
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/post-it-icon.png"
    }
  }
}