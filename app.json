{
  "expo": {
    "name": "The Decider Skills",
    "slug": "The-Decider-Skills",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/post-it-icon.png",
    "splash": {
      "image": "./assets/post-it-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.silent2code.handbook"
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/post-it-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.silent2code.handbook"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-firebase/crashlytics"
    ],
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/post-it-icon.png"
    }
  }
}